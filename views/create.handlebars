<h1>NEW TEAM</h1>
<div>
  <form action="/create" method="POST" enctype="multipart/form-data">
    <br>
    <div>
      <label for="imagen">PIC ➨ </label>
      <input type="file" name="imagen" id="imagen">
      <input type="submit" class="submit" value="Upload" />
    </div>
<br>
    <div>
      <label>Name</label>
      <input type="text" name="name" placeholder="Name">
    </div> 
    <div>
      <label>ShortName</label>
      <input type="text" name="shortName" placeholder="Shortname">
    </div>
    <div>

      <label>TLA</label>
      <input type="text" name="tla" placeholder="TLA">
    </div>
    <div>

      <label>Address</label>
      <input type="text" name="address" placeholder="Address">
    </div>
    <div>
      <label>Tel</label>
      <input type="text" name="phone" placeholder="phone">
    </div>
    <div>
      <label>Website</label>
      <input type="text" name="website" placeholder="website">
    </div>
    <div>
      <label>E-Mail</label>
      <input type="text" name="email" placeholder="email">
    </div>
    <div>
      <label>Founded</label>
      <input type="text" name="founded" placeholder="Founded">
    </div>
    <div>
      <label>Colors</label>
      <input type="text" name="clubColors" placeholder="Colors">
    </div>
    <div>
      <label>Estadio</label>
      <input type="text" name="venue" placeholder="Venue">
    </div> <br>
    <div>
      <button type="submit" class="submit">Create Team</button>
    </div>
  </form>
</div>
